if ($request_method = 'OPTIONS') {
add_header 'Access-Control-Allow-Origin' "${scheme}://ventas-prueba.loteria.com.ec" always;
add_header 'Access-Control-Allow-Methods' 'POST, OPTIONS, GET, DELETE, PUT';
add_header 'Access-Control-Max-Age' 1728000;
add_header 'Content-Type' 'application/json';
return 200
}
if ($request_method = 'POST') {
add_header 'Access-Control-Allow-Origin' "${scheme}://ventas-prueba.loteria.com.ec" always;
add_header 'Access-Control-Allow-Methods' 'POST';
}
if ($request_method = 'GET') {
add_header 'Access-Control-Allow-Origin' "${scheme}://ventas-prueba.loteria.com.ec" always;
add_header 'Access-Control-Allow-Methods' 'GET';
}
if ($request_method = 'DELETE') {
add_header 'Access-Control-Allow-Origin' "${scheme}://ventas-prueba.loteria.com.ec" always;
add_header 'Access-Control-Allow-Methods' 'DELETE';
}
if ($request_method = 'PUT') {
add_header 'Access-Control-Allow-Origin' "${scheme}://ventas-prueba.loteria.com.ec" always;
add_header 'Access-Control-Allow-Methods' 'PUT';
}