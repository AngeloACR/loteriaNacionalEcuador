let LottoBoletinComponent = class LottoBoletinComponent {
    constructor(actRoute, router, inquiryService) {
        this.actRoute = actRoute;
        this.router = router;
        this.inquiryService = inquiryService;
        this.imgNotFound = false;
        this.showBox = false;
        this.isLoading = false;
        this.loadingMessage = "Buscando el boletin";
        this.actRoute.params.subscribe(params => {
            this.sorteo = params["sorteo"];
        });
    }
    ngOnInit() {
        return tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"](this, void 0, void 0, function* () {
            /* console.log("En la consulta de loteria"); */
            this.triggerLoader();
            this.router.routeReuseStrategy.shouldReuseRoute = () => false;
            this.boletinImagen = yield this.inquiryService.obtenerBoletin(2, this.sorteo);
            this.showBox = true;
            this.dismissLoader();
        });
    }
    handleImgError() {
        console.log("Img not found");
        this.imgNotFound = true;
    }
    triggerLoader() {
        this.isLoading = true;
    }
    dismissLoader() {
        this.isLoading = false;
    }
