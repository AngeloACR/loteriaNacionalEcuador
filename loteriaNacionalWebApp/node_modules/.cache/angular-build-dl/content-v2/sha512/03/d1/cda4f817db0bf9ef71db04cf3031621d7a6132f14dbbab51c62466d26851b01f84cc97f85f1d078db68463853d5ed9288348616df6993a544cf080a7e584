                let combinaciones = [{ principal: this.combinacion, serie: this.serie }];
                let data = yield this.inquiryService.recuperarBoletoGanador(14, this.sorteoGanador, combinaciones);
                this.resultados.emit(data);
                this.dismissLoader();
            }
            catch (e) {
                this.dismissLoader();
                this.openError(e.message);
                console.log(e);
